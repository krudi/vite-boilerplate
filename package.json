{
  "name": "vite-boilerplate",
  "description": "A boilerplate with Vue3, Nuxt3, SASS and technology such as Vite",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/krudi/vite-boilerplate/LICENSE/"
    }
  ],
  "version": "1.0.0",
  "authors": [
    {
      "name": "authors",
      "email": "example@mail.com",
      "url": "https://github.com/krudi/vite-boilerplate/"
    }
  ],
  "private": false,
  "main": "nuxt.config.js",
  "homepage": "https://github.com/krudi/vite-boilerplate#readme/",
  "bugs": {
    "url": "https://github.com/krudi/vite-boilerplate/issues/"
  },
  "keywords": [
    "boilerplate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krudi/vite-boilerplate.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "http://example.com/donate/"
    }
  ],
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src,dist}/**/*.{js,map}"
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "dev": "nuxi dev && lint",
    "watch": "node .output/server/index.mjs",
    "prod": "nuxi build",
    "lint:js": "eslint --ext .js,.vue pages/ components/ assets/js",
    "lint:js:fix": "eslint --ext .js,.vue --fix pages/ components/",
    "lint:css": "stylelint .css,.scss,.vue pages/ components/ assets/js",
    "lint:css:fix": "stylelint .css,.scss,.vue --fix pages components assets/scss",
    "lint:files": "npx prettier --write .",
    "test": "yarn jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.37",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.9",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "10.4.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^7.1.5",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-vue": "^8.5.0",
    "nuxt-purgecss": "^0.2.1",
    "nuxt3": "3.0.0-27409835.c53c736",
    "postcss": "^8.4.6",
    "prettier": "^2.7.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.1.1",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "vue-loader": "^16.8.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^5.1.3",
    "jest": "^27.5.1",
    "markdownlint": "^0.25.1",
    "postcss-html": "^1.4.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "transform-class-properties": "^1.0.0-beta",
    "vite": "^2.8.6",
    "vue": "^3.2.30"
  }
}
